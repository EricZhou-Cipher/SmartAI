+    if (needsRestart) {
+      logger.error('ğŸ”¥ æœåŠ¡å™¨æ€§èƒ½ä¸¥é‡æ¶åŒ–ï¼Œå‡†å¤‡é‡å¯åº”ç”¨', {
+        cpuLoad: avgCpu,
+        memoryUsage: avgMem,
+        eventLoopLag: avgLag,
+        errorRate: stats.errorRate,
+        p95Latency: stats.p95Latency,
+      });
+
+      // è®°å½•ä¼˜åŒ–æ—¶é—´
+      stats.lastOptimization = Date.now();
