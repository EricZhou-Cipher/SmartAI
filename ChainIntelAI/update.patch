+    // æ£€æŸ¥æ˜¯å¦éœ€è¦é™çº§API
+    const needsDegradation =
+      avgCpu > PERFORMANCE_THRESHOLDS.CPU_HIGH ||
+      avgMem > PERFORMANCE_THRESHOLDS.MEMORY_HIGH ||
