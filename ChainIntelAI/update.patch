+    totalValue: (BigInt(transaction.value) * BigInt(Math.floor(Math.random() * 10) + 1)).toString(),
+  };
+
+  // æ·»åŠ æ ‡ç­¾
+  transaction.tags = [];
+  const possibleTags = ['exchange', 'defi', 'nft', 'gaming', 'bridge'];
