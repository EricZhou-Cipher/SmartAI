{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env PACKAGE_VERSION=$npm_package_version next dev",
    "build": "cross-env PACKAGE_VERSION=$npm_package_version next build",
    "start": "cross-env PACKAGE_VERSION=$npm_package_version next start",
    "lint": "next lint",
    "format": "prettier --write 'app/**/*.{js,jsx,ts,tsx,json,css,md}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:component": "cypress open --component",
    "cypress:component:headless": "cypress run --component",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "test:all": "yarn test && yarn test:api && yarn cypress:headless",
    "test:api": "jest tests/api",
    "type-check": "tsc --noEmit",
    "test:component": "jest --testPathPattern=__tests__/components/",
    "create-test": "./scripts/create-test.sh",
    "run-tests": "./scripts/run-tests.sh"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "autoprefixer": "^10.4.17",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.8",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "next": "^14.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@cypress/react": "^9.0.0",
    "@cypress/webpack-dev-server": "^4.0.1",
    "@eslint/eslintrc": "^2.1.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "axe-core": "^4.10.3",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cypress": "^14.2.0",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.6",
    "typescript": "^5.3.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "main": "index.js",
  "license": "MIT"
}
